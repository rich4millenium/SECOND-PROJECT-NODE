<h2 id='scores'>High Scores</h2>
<div class="container">
  <div class="row">
    <div class="col-sm-4">
{{#each score}}
{{this.name}} - {{this.score}} <br>
<hr>
{{/each}}
    </div>
</div>
</div>
<form method="POST" action="/submit">
